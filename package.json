    "@types/node": "^24.10.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.39.0",
    "eslint-plugin-oxlint": "^0.16.12",
    "eslint-plugin-vue": "~10.0.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^7.0.2",
